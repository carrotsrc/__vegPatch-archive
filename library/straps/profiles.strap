<table name="profiles">
	<field name="id" type="int(11)" extra="NOT NULL AUTO_INCREMENT" />
	<field name="first" type="varchar(16)" extra="NOT NULL" />
	<field name="surname" type="varchar(16)" extra="NOT NULL" />
	<field name="status" type="varchar(255)" extra="NOT NULL" />
	<field name="avatar" type="char(32)" extra="NOT NULL" />
	<primary name="id" />
</table>


<rtype name="Profile" base="entity" />
